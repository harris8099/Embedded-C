#include <stdio.h>

extern float add(float a , float b);
extern float sub(float a , float b);
extern float multi(float a , float b);
extern float div(float a , float b);

int main(){
    printf("add is: %f\n", add(1.0,3.0));
    printf("sub is: %f\n", sub(1.0,3.0));
    printf("multi is: %f\n", multi(1.0,3.0));
    printf("div is: %f\n", div(1.0,3.0));
    return 0;
}

